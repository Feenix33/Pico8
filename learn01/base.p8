function _init()
    -- print a runtime note
    local div,m
    m=stat(94)
    if m < 10 then div=":0" else div=":" end
    printh (stat(93)..div..stat(94).." ---------------------------------------- ")

end

function _update()

    if btnp(0) then
        printh("Btn 0 left")

    elseif btnp(1) then
        printh("Btn 1 right")

    elseif btnp(2) then 
        printh("Btn 2")

    elseif btnp(3) then --printh("Btn 3 left")
        printh("Btn 3 down")

    elseif btnp(4) then
        printh("Btn 4")

    elseif btnp(5) then
        printh("button 5")
    end
end

function _draw()
    cls()
    rect(0, 0, 127, 127, 6)
end

